                        <Image Grid.Column="6" Grid.Row="1" x:Name="IMG_Conn_AAD"  HorizontalAlignment="Center" VerticalAlignment="Center" Source="C:\Dev\M365 Inventory Automation (Metro)\M365 Inventory Automation (Metro)\Resources\Icons\Light.Red.ico"/>
                        <Label Grid.Column="7" Grid.Row="1" Content="Connection to Azure Active Directory"/>
                        <!-- Conn EXO -->
                        <Image Grid.Column="6" Grid.Row="3" x:Name="IMG_Conn_EXO"  HorizontalAlignment="Center" VerticalAlignment="Center" Source="C:\Dev\M365 Inventory Automation (Metro)\M365 Inventory Automation (Metro)\Resources\Icons\Light.Red.ico"/>
                        <Label Grid.Column="7" Grid.Row="3" Content="Connection to Exchange Online"/>
                        <!-- Conn AAD -->
                        <Image Grid.Column="6" Grid.Row="5" x:Name="IMG_Conn_SPO"  HorizontalAlignment="Center" VerticalAlignment="Center" Source="C:\Dev\M365 Inventory Automation (Metro)\M365 Inventory Automation (Metro)\Resources\Icons\Light.Red.ico"/>
                        <Label Grid.Column="7" Grid.Row="5" Content="Connection to SharePoint Online"/>
                        <!-- Conn MSOL -->
                        <Image Grid.Column="6" Grid.Row="7" x:Name="IMG_Conn_MSOL"  HorizontalAlignment="Center" VerticalAlignment="Center" Source="C:\Dev\M365 Inventory Automation (Metro)\M365 Inventory Automation (Metro)\Resources\Icons\Light.Red.ico"/>
                        <Label Grid.Column="7" Grid.Row="7" Content="Connection to MS Online"/>
                        <!-- Report SPO All Sites -->
                        <Image Grid.Column="6" Grid.Row="9" x:Name="IMG_Report_AllSites"  HorizontalAlignment="Center" VerticalAlignment="Center" Source="C:\Dev\M365 Inventory Automation (Metro)\M365 Inventory Automation (Metro)\Resources\Icons\Light.Orange.ico"/>
                        <Label Grid.Column="7" Grid.Row="9" Content="SharePoint Online - All Sites"/>
                        <!-- Report SPO All Site Groups -->
                        <Image Grid.Column="6" Grid.Row="11" x:Name="IMG_Report_AllSiteGroups"  HorizontalAlignment="Center" VerticalAlignment="Center" Source="C:\Dev\M365 Inventory Automation (Metro)\M365 Inventory Automation (Metro)\Resources\Icons\Light.Orange.ico"/>
                        <Label Grid.Column="7" Grid.Row="11" Content="SharePoint Online - All Site groups"/>
                        <!-- Report SPO Libraries -->
                        <Image Grid.Column="6" Grid.Row="13" x:Name="IMG_Report_Libraries"  HorizontalAlignment="Center" VerticalAlignment="Center" Source="C:\Dev\M365 Inventory Automation (Metro)\M365 Inventory Automation (Metro)\Resources\Icons\Light.Orange.ico"/>
                        <Label Grid.Column="7" Grid.Row="13" Content="SharePoint Online - Libraries"/>
                        <!-- Report AAD Users -->
                        <Image Grid.Column="6" Grid.Row="15" x:Name="IMG_Report_AADUsers"  HorizontalAlignment="Center" VerticalAlignment="Center" Source="C:\Dev\M365 Inventory Automation (Metro)\M365 Inventory Automation (Metro)\Resources\Icons\Light.Orange.ico"/>
                        <Label Grid.Column="7" Grid.Row="15" Content="Azure Active Directory - Users"/>
                        <!-- Report AAD Deleted Users -->
                        <Image Grid.Column="6" Grid.Row="17" x:Name="IMG_Report_AADDelusers"  HorizontalAlignment="Center" VerticalAlignment="Center" Source="C:\Dev\M365 Inventory Automation (Metro)\M365 Inventory Automation (Metro)\Resources\Icons\Light.Orange.ico"/>
                        <Label Grid.Column="7" Grid.Row="17" Content="Azure Active Directory - Deleted Users"/>
                        <!-- Report AAD Contacts -->
                        <Image Grid.Column="6" Grid.Row="19" x:Name="IMG_Report_AADContacts"  HorizontalAlignment="Center" VerticalAlignment="Center" Source="C:\Dev\M365 Inventory Automation (Metro)\M365 Inventory Automation (Metro)\Resources\Icons\Light.Orange.ico"/>
                        <Label Grid.Column="7" Grid.Row="19" Content="Azure Active Directory - Contacts"/>
                        <!-- Report AAD Groups -->
                        <Image Grid.Column="6" Grid.Row="21" x:Name="IMG_Report_AADGroups"  HorizontalAlignment="Center" VerticalAlignment="Center" Source="C:\Dev\M365 Inventory Automation (Metro)\M365 Inventory Automation (Metro)\Resources\Icons\Light.Orange.ico"/>
                        <Label Grid.Column="7" Grid.Row="21" Content="Azure Active Directory - Groups"/>
                        <!-- Report AAD Domains -->
                        <Image Grid.Column="6" Grid.Row="23" x:Name="IMG_Report_AADDomains"  HorizontalAlignment="Center" VerticalAlignment="Center" Source="C:\Dev\M365 Inventory Automation (Metro)\M365 Inventory Automation (Metro)\Resources\Icons\Light.Orange.ico"/>
                        <Label Grid.Column="7" Grid.Row="23" Content="Azure Active Directory - Domains"/>
                        <!-- Report EXO Mailboxes -->
                        <Image Grid.Column="6" Grid.Row="25" x:Name="IMG_Report_EXOMailboxes"  HorizontalAlignment="Center" VerticalAlignment="Center" Source="C:\Dev\M365 Inventory Automation (Metro)\M365 Inventory Automation (Metro)\Resources\Icons\Light.Orange.ico"/>
                        <Label Grid.Column="7" Grid.Row="25" Content="Exchange Online - Mailboxes"/>
                        <!-- Report EXO Archives -->
                        <Image Grid.Column="6" Grid.Row="27" x:Name="IMG_Report_EXOArchives"  HorizontalAlignment="Center" VerticalAlignment="Center" Source="C:\Dev\M365 Inventory Automation (Metro)\M365 Inventory Automation (Metro)\Resources\Icons\Light.Orange.ico"/>
                        <Label Grid.Column="7" Grid.Row="27" Content="Exchange Online - Archives"/>
                        <!-- Report EXO groups -->
                        <Image Grid.Column="9" Grid.Row="1" x:Name="IMG_Report_EXOGroups"  HorizontalAlignment="Center" VerticalAlignment="Center" Source="C:\Dev\M365 Inventory Automation (Metro)\M365 Inventory Automation (Metro)\Resources\Icons\Light.Orange.ico"/>
                        <Label Grid.Column="10" Grid.Row="1" Content="Exchange Online - Groups"/>
                        <!-- Report Azure Graph Reports -->
                        <Image Grid.Column="9" Grid.Row="3" x:Name="IMG_Report_Graph"  HorizontalAlignment="Center" VerticalAlignment="Center" Source="C:\Dev\M365 Inventory Automation (Metro)\M365 Inventory Automation (Metro)\Resources\Icons\Light.Orange.ico"/>
                        <Label Grid.Column="10" Grid.Row="3" Content="Azure Graph - All Reports"/>
                        <!-- END ProgressItems -->
                        <!-- Report SPO Shared files and sites -->
                        <Image Grid.Column="9" Grid.Row="5" x:Name="IMG_Report_SPOShared"  HorizontalAlignment="Center" VerticalAlignment="Center" Source="C:\Dev\M365 Inventory Automation (Metro)\M365 Inventory Automation (Metro)\Resources\Icons\Light.Orange.ico"/>
                        <Label Grid.Column="10" Grid.Row="5" Content="Shared files and sites" />
                        <!-- END  -->
                        <!-- Report External users  -->
                        <Image Grid.Column="9" Grid.Row="7" x:Name="IMG_Report_ExternalUsers"  HorizontalAlignment="Center" VerticalAlignment="Center" Source="C:\Dev\M365 Inventory Automation (Metro)\M365 Inventory Automation (Metro)\Resources\Icons\Light.Orange.ico"/>
                        <Label Grid.Column="10" Grid.Row="7" Content="External Users" />
                        <!-- END  -->
                        <!-- Report Microsoft Flows -->
                        <Image Grid.Column="9" Grid.Row="9" x:Name="IMG_Report_Flow"  HorizontalAlignment="Center" VerticalAlignment="Center" Source="C:\Dev\M365 Inventory Automation (Metro)\M365 Inventory Automation (Metro)\Resources\Icons\Light.Orange.ico"/>
                        <Label Grid.Column="10" Grid.Row="9" Content="Microsoft Flows" />
                        <!-- END  -->
